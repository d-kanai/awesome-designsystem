{
  "name": "@awesome/design-system",
  "version": "0.1.0",
  "description": "Design system components from Figma",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "figma:connect:publish": "export $(cat .env 2>/dev/null | xargs) && figma connect publish",
    "figma:connect:publish:dir": "export $(cat .env 2>/dev/null | xargs) && figma connect publish --dir",
    "figma:connect:unpublish": "export $(cat .env 2>/dev/null | xargs) && figma connect unpublish",
    "figma:connect:unpublish:node": "export $(cat .env 2>/dev/null | xargs) && figma connect unpublish --label React --node",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=4.0.0"
  },
  "devDependencies": {
    "@figma/code-connect": "1.3.10",
    "@testing-library/dom": "10.4.0",
    "@storybook/react": "10.0.8",
    "@storybook/react-vite": "10.0.8",
    "@types/node": "22.19.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "clsx": "2.1.1",
    "next": "16.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "storybook": "10.0.8",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.17",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vite": "6.3.5"
  },
  "packageManager": "pnpm@10.10.0"
}
